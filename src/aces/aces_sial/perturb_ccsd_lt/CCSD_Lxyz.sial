#---AAA
     PARDO i1, a1, a, i
            Tiaai(i1,a1,a,i)            = 0.0
            PREPARE W2mebj_aa_x(i1,a1,a,i) = Tiaai(i1,a1,a,i)
      ENDPARDO i1, a1, a, i

      PARDO i, a, a2, i2
            REQUEST T2old_aa_x(a,i2,a2,i) i
            DO i1
            DO a1
               REQUEST                         VSpipi(a1,i1,a2,i2) i2 # + 
               Tiaai(i1,a1,a,i)              = VSpipi(a1,i1,a2,i2)*T2old_aa_x(a,i2,a2,i)
               Tiaai(i1,a1,a,i)             *= -1.0
               PREPARE W2mebj_aa_x(i1,a1,a,i) += Tiaai(i1,a1,a,i)                  #PV be carefull
            ENDDO a1
            ENDDO i1
      ENDPARDO i, a, a2, i2
#...................................
      PARDO a, i, b, j
            REQUEST T2old_ab_x(a,i,b,j) j
            DO i1
            DO a1
               REQUEST                         Vpiqj(a1,i1,b,j) j # + 
               Tiaai(i1,a1,a,i)              = Vpiqj(a1,i1,b,j)*T2old_ab_x(a,i,b,j)
               PREPARE W2mebj_aa_x(i1,a1,a,i) += Tiaai(i1,a1,a,i)               #PV be careful
            ENDDO a1
            ENDDO i1
      ENDPARDO a, i, b, j

#----AABB

      PARDO i, a, b, j
            Tiabj(i,a,b,j)             = 0.0
            PREPARE W2mebj_ab_x(i,a,b,j) = Tiabj(i,a,b,j)
      ENDPARDO i, a, b, j

      PARDO b, j, a1, i1 
            REQUEST T2old_ab_x(a1,i1,b,j) j 
            DO a
            DO i
               REQUEST                       VSpipi(a,i,a1,i1) i1 # +  
               Tiabj(i,a,b,j)              = VSpipi(a,i,a1,i1)*T2old_ab_x(a1,i1,b,j)
               PREPARE W2mebj_ab_x(i,a,b,j) += Tiabj(i,a,b,j) 
            ENDDO i
            ENDDO a
      ENDPARDO b, j, a1, i1 
#....................................
      PARDO b, j, b1, j1 
            REQUEST T2old_bb_x(b1,j1,b,j) j 
            DO a
            DO i
               REQUEST                       Vpiqj(a,i,b1,j1)    j1 # +  
               Tiabj(i,a,b,j)              = Vpiqj(a,i,b1,j1)*T2old_bb_x(b1,j1,b,j)
               PREPARE W2mebj_ab_x(i,a,b,j) += Tiabj(i,a,b,j)
            ENDDO i
            ENDDO a
      ENDPARDO b, j, b1, j1

#---BBAA
      PARDO j, b, a, i
            Tjbai(j,b,a,i)             = 0.0
            PREPARE W2mebj_ba_x(j,b,a,i) = Tjbai(j,b,a,i)
      ENDPARDO j, b, a, i

      PARDO a, i, b1, j1 
            REQUEST T2old_ab_x(a,i,b1,j1) j1
            DO b
            DO j
               REQUEST                         VSqjqj(b,j,b1,j1)   j1 # +  
               Tjbai(j,b,a,i)                = VSqjqj(b,j,b1,j1)*T2old_ab_x(a,i,b1,j1)
               PREPARE W2mebj_ba_x(j,b,a,i) += Tjbai(j,b,a,i)  # verify + sign in this PV
            ENDDO j
            ENDDO b
      ENDPARDO a, i, b1, j1 
#.......................................

      PARDO a, i, a1, i1
            REQUEST T2old_aa_x(a1,i1,a,i) i
            DO b
            DO j
               REQUEST                         Vpiqj(a1,i1,b,j)    j # +  
               Tjbai(j,b,a,i)                = Vpiqj(a1,i1,b,j)*T2old_aa_x(a1,i1,a,i)
               PREPARE W2mebj_ba_x(j,b,a,i) += Tjbai(j,b,a,i)    # verify + sign in this PV
            ENDDO j
            ENDDO b
      ENDPARDO a, i, a1, i1 

#---BBBB

      PARDO j1, b1, b, j
            Tjbbj(j1,b1,b,j)            = 0.0
            PREPARE W2mebj_bb_x(j1,b1,b,j) = Tjbbj(j1,b1,b,j)
      ENDPARDO j1, b1, b, j

      PARDO b, j, b2, j2 
            REQUEST T2old_bb_x(b,j2,b2,j) j
            DO j1
            DO b1
               REQUEST                         VSqjqj(b1,j1,b2,j2) j2 # + 
               Tjbbj(j1,b1,b,j)              = VSqjqj(b1,j1,b2,j2)*T2old_bb_x(b,j2,b2,j)
               Tjbbj(j1,b1,b,j)             *= -1.0
               PREPARE W2mebj_bb_x(j1,b1,b,j) += Tjbbj(j1,b1,b,j)
            ENDDO b1
            ENDDO j1
      ENDPARDO b, j, b2, j2 

      PARDO b, j, a, i 
            REQUEST T2old_ab_x(a,i,b,j) j 
            DO j1
            DO b1
               REQUEST                         Vpiqj(a,i,b1,j1)  j1 # + 
               Tjbbj(j1,b1,b,j)              = Vpiqj(a,i,b1,j1)*T2old_ab_x(a,i,b,j)
               PREPARE W2mebj_bb_x(j1,b1,b,j) += Tjbbj(j1,b1,b,j)
            ENDDO b1
            ENDDO j1
      ENDPARDO b, j, a, i 
