           t2jbjb(j,b,j1,b1)            = D0jbjb(j,b,j1,b1)  
           t2jbjb(j,b,j1,b1)           += e1jbjb(j,b,j1,b1)
           t2jbjb(j,b,j1,b1)           *= c1
           tjbjb(j,b,j1,b1)             = t2jbjb(j,b,j1,b1)
