               REQUEST L2old_aa_x(i,a,i1,a1) a1

               T1iajb(i1,a1,j,b)            = L2old_aa_x(i,a,i1,a1)*W1MEBJ_BA(j,b,a,i)
               PREPARE L2new_ab_x(i1,a1,j,b) += T1iajb(i1,a1,j,b)
            ENDDO a1
