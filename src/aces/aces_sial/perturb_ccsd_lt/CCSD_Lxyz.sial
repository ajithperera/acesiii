                  T1aiai(a2,i,a,i2)  += T2old_aa_x(a2,i,a,i2)

                  tai(a,i2)           = t1a_old(a,i2)
                  T2aiai(a2,i,a,i2)   = t1a_old_x(a2,i)^tai(a,i2)
                  T1aiai(a2,i,a,i2)  += T2aiai(a2,i,a,i2)

                  T1iaai(i1,a1,a,i)   = T1aiai(a2,i,a,i2)*Taiai(a1,i2,a2,i1)
