     PROC L2NEWBB_x
#   ---------------
        PARDO b, b1, b2, j1              
            REQUEST VSbbbj(b,b2,b1,j1)  j1
            DO j
               GET                            l1b_old_x(j,b2)
               Tjbjb(j,b,j1,b1)             = VSbbbj(b,b2,b1,j1)*l1b_old_x(j,b2)
               T3jbjb(j1,b,j,b1)            = Tjbjb(j,b,j1,b1)
               T3jbjb(j1,b,j,b1)           *= -1.0
               PREPARE L2new_bb_x(j,b,j1,b1) +=  Tjbjb(j,b,j1,b1)
               PREPARE L2new_bb_x(j1,b,j,b1) += T3jbjb(j1,b,j,b1)
            ENDDO j
        ENDPARDO b, b1, b2, j1
#.......................................................................
        PARDO b, j, j1, b1                   
            REQUEST             VSqjqj(b1,j1,b,j) j
            GET                 l1b_old_x(j1,b1)
            GET                 Fme_b(j,b)
#PV            Tjbjb(j1,b1,j,b)  = VSqjqj(b1,j1,b,j)
             Tjbjb(j1,b1,j,b)  =0.0
            T2jbjb(j,b1,j1,b) = 0.0
            T3jbjb(j1,b,j,b1) = 0.0
            T4jbjb(j,b,j1,b1) = 0.0
            DO b2
               REQUEST              L2old_bb_x(j1,b1,j,b2) b2
               REQUEST              VSqjqj(b1,j1,b2,j)   j
               GET                  Gae_b_x(b,b2)
               GET                  F1ae_b(b2,b)
               GET                  l1b_old(j1,b2)

               T1jbjb(j1,b1,j,b)  = L2old_bb_x(j1,b1,j,b2)*F1ae_b(b2,b)
               Tjbjb(j1,b1,j,b)  += T1jbjb(j1,b1,j,b)

               Tbb(b2,b)          = Gae_b_x(b,b2)
               TXjbjb(j1,b,j,b1)  = T1jbjb(j1,b1,j,b)
               T3jbjb(j1,b,j,b1) -= TXjbjb(j1,b,j,b1)
               DO j2
                  GET          l1b_old_x(j2,b)
                  GET          t1b_old(b2,j2)

                  T1bb(b2,b) = t1b_old(b2,j2)*l1b_old_x(j2,b)
                  Tbb(b2,b) -= T1bb(b2,b)
               ENDDO j2
               T1jbjb(j1,b1,j,b)  = VSqjqj(b1,j1,b2,j)*Tbb(b2,b)
               Tjbjb(j1,b1,j,b)  += T1jbjb(j1,b1,j,b)

               TXjbjb(j1,b,j,b1)  = T1jbjb(j1,b1,j,b)
               T3jbjb(j1,b,j,b1) -= TXjbjb(j1,b,j,b1)
            ENDDO b2
            T1jbjb(j1,b1,j,b)  = l1b_old_x(j1,b1)^Fme_b(j,b)
            Tjbjb(j1,b1,j,b)  += T1jbjb(j1,b1,j,b)

            TXjbjb(j1,b,j,b1)  = T1jbjb(j1,b1,j,b)
            T3jbjb(j1,b,j,b1) -= TXjbjb(j1,b,j,b1)

            TXjbjb(j,b1,j1,b)  = T1jbjb(j1,b1,j,b)
            T2jbjb(j,b1,j1,b) -= TXjbjb(j,b1,j1,b)

            TXjbjb(j,b,j1,b1)  = T1jbjb(j1,b1,j,b)
            T4jbjb(j,b,j1,b1) += TXjbjb(j,b,j1,b1)

            PREPARE L2NEW_bb_x(j1,b1,j,b) +=  Tjbjb(j1,b1,j,b)
            PREPARE L2NEW_bb_x(j,b1,j1,b) += T2jbjb(j,b1,j1,b)
            PREPARE L2NEW_bb_x(j1,b,j,b1) += T3jbjb(j1,b,j,b1)
            PREPARE L2NEW_bb_x(j,b,j1,b1) += T4jbjb(j,b,j1,b1)
      ENDPARDO b, j, j1, b1
#.............................................................
       PARDO j, b1, b, j1
            Tjbjb(j1,b1,j,b)  = 0.0
            T2jbjb(j,b1,j1,b) = 0.0
            T3jbjb(j1,b,j,b1) = 0.0
            DO j2

               REQUEST              L2old_bb_x(j1,b1,j2,b) b
               REQUEST              VSqjqj(j2,j1,b,j)    j
               REQUEST              VSqjqj(b1,j1,b,j2)   j2
               GET                  Gmi_b_x(j2,j)
               GET                  F1mi_b(j,j2)
               GET                  l1b_old_x(j2,b1)

               T1jbjb(j1,b1,j,b)  = L2old_bb_x(j1,b1,j2,b)*F1mi_b(j,j2)
               T1jbjb(j1,b1,j,b) *= -1.0
               Tjbjb(j1,b1,j,b)  += T1jbjb(j1,b1,j,b)

               Tjj(j,j2)          = Gmi_b_x(j2,j)
               TXjbjb(j,b1,j1,b)  = T1jbjb(j1,b1,j,b)
               T2jbjb(j,b1,j1,b) -= TXjbjb(j,b1,j1,b)

               T1jbjb(j1,b1,j,b)  = VSqjqj(j2,j1,b,j)*l1b_old_x(j2,b1)
               Tjbjb(j1,b1,j,b)  -= T1jbjb(j1,b1,j,b)

               TXjbjb(j1,b,j,b1)  = T1jbjb(j1,b1,j,b)
               T3jbjb(j1,b,j,b1) += TXjbjb(j1,b,j,b1)

               DO b2

                  GET          l1b_old_x(j,b2)
                  GET          t1b_old(b2,j2)

                  T1jj(j,j2) = l1b_old_x(j,b2)*t1b_old(b2,j2)
                  Tjj(j,j2) += T1jj(j,j2)
               ENDDO b2
               T1jbjb(j1,b1,j,b)  = VSqjqj(b1,j1,b,j2)*Tjj(j,j2)
               T1jbjb(j1,b1,j,b) *= -1.0
               Tjbjb(j1,b1,j,b)  += T1jbjb(j1,b1,j,b)

               TXjbjb(j,b1,j1,b)  = T1jbjb(j1,b1,j,b)
               T2jbjb(j,b1,j1,b) -= TXjbjb(j,b1,j1,b)
               DO j3
                  REQUEST              L2old_bb_x(j2,b1,j3,b)  b
                  GET                  W1minj_bb(j1,j2,j,j3)
                  T1jbjb(j1,b1,j,b)  = L2old_bb_x(j2,b1,j3,b)*W1minj_bb(j1,j2,j,j3)
                  T1jbjb(j1,b1,j,b) *= 0.5
                  Tjbjb(j1,b1,j,b)  += T1jbjb(j1,b1,j,b)
               ENDDO j3
            ENDDO j2
            PREPARE L2NEW_bb_x(j1,b1,j,b) +=  Tjbjb(j1,b1,j,b)
            PREPARE L2NEW_bb_x(j,b1,j1,b) += T2jbjb(j,b1,j1,b)
            PREPARE L2NEW_bb_x(j1,b,j,b1) += T3jbjb(j1,b,j,b1)
      ENDPARDO j, b1, b, j1

    ENDPROC L2NEWBB_x
