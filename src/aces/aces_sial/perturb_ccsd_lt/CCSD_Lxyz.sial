               REQUEST VSqjqj(b1,j1,b,j)   j
               T1jbai(j,b,a,i) = T2old_ab_x(a,i,b1,j1)*VSqjqj(b1,j1,b,j)
               Tjbai(j,b,a,i) += T1jbai(j,b,a,i)
            ENDDO j1
            ENDDO b1
            PREPARE W1MEBJ_BA_x(j,b,a,i) = Tjbai(j,b,a,i)
      ENDPARDO i, a, j, b

      ENDPROC W1MEBJBA_S_x
