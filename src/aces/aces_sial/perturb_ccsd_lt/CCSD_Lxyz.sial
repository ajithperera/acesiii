      PROC L1BOLD_X
#     -------------
      PARDO b, j                  
            GET                 Fme_b_x(j,b)
            Tjb(j,b)          = Fme_b_x(j,b)                
            PUT l1b_old_x(j,b) += Tjb(j,b)
      ENDPARDO b, j
#................................
      PARDO b, j, j1                
            GET                 l1b_old(j1,b)
            GET                 F1mi_b_x(j,j1)
            Tjb(j,b)          = l1b_old(j1,b)*F1mi_b_x(j,j1)
            Tjb(j,b)         *= -1.0  
            PUT l1b_old_x(j,b) += Tjb(j,b)
      ENDPARDO b, j, j1 
#................................................................
#pv      PARDO b, j, j1, j2   
#pv            REQUEST             VSqjqj(b,j,j1,j2) j2  
#pv            GET                 Gmi_b(j2,j1)
#pv            Tjb(j,b)          = VSqjqj(b,j,j1,j2)*Gmi_b(j2,j1)
#pv            Tjb(j,b)         *= -1.0  
#pv            PUT l1b_new(j,b) += Tjb(j,b)
#pv      ENDPARDO b, j, j1, j2   
#................................................................
      PARDO b, b1, j1, j2 
            REQUEST L2old_bb(j1,b,j2,b1)  b1 
            DO j
               REQUEST             W1imen_bb_x(j,j1,b1,j2) j2 
               Tjb(j,b)          = W1imen_bb_x(j,j1,b1,j2)*L2old_bb(j1,b,j2,b1)
               Tjb(j,b)         *= -0.5
               PUT l1b_old_x(j,b) += Tjb(j,b)
            ENDDO j
      ENDPARDO b, b1, j1, j2  
#...............................
      PARDO j, b, b1   
            GET                 l1b_old(j,b1)
            GET                 F1ae_b_x(b1,b)
            Tjb(j,b)          = l1b_old(j,b1)*F1ae_b_x(b1,b)
            PUT l1b_old_x(j,b) += Tjb(j,b)
      ENDPARDO j, b, b1  
#.....................................................................
#pv      PARDO b, j, b1, b2 
#pv            REQUEST             VSbbbj(b2,b1,b,j) j
#pv            GET                 Gae_b(b1,b2)
#pv         Tjb(j,b)          = VSbbbj(b2,b1,b,j)*Gae_b(b1,b2)
#pv         Tjb(j,b)         *= -1.0  
#pv         PUT l1b_new(j,b) += Tjb(j,b)
#pv   ENDPARDO b, j, b1, b2  
#pv      PARDO b, j, i, i1   
#pv            REQUEST             Vpiqj(i,i1,b,j) j
#pv            GET                 Gmi_a(i1,i)
#pv            Tjb(j,b)          = Vpiqj(i,i1,b,j)*Gmi_a(i1,i)
#pv            Tjb(j,b)         *= -1.0  
#pv            PUT l1b_new(j,b) += Tjb(j,b)
#pv      ENDPARDO b, j, i, i1   
#...................................................................
      PARDO b, j1, a, i  
            REQUEST L2old_ab(i,a,j1,b) b 
            DO j
               REQUEST             W1imen_ba_x(j,j1,a,i) i 
               Tjb(j,b)          = W1imen_ba_x(j,j1,a,i)*L2old_ab(i,a,j1,b)
               Tjb(j,b)         *= -1.0  
              PUT l1b_old_x(j,b) += Tjb(j,b)
            ENDDO j
      ENDPARDO b, j1, a, i  
#.....................................................................
#pv      PARDO b, j, a, a1 
#pv            REQUEST             Vaabj(a1,a,b,j) j
#pv            GET                 Gae_a(a,a1)
#pv            Tjb(j,b)          = Vaabj(a1,a,b,j)*Gae_a(a,a1)
#pv            Tjb(j,b)         *= -1.0  
#pv            PUT l1b_new(j,b) += Tjb(j,b)
#pv      ENDPARDO b, j, a, a1 
#....................................................................
      PARDO b, b1, j, j1
            REQUEST VSqjqj(b,j,b1,j1) j1  
            Tbjbj(b,j1,b1,j) = VSqjqj(b,j,b1,j1)
            T2jb(j1,b1) = 0.0
            DO b2
               GET Gae_b(b2,b1)
               GET t1b_old_x(b2,j1)
               T3jb(j1,b1)  = Gae_b(b2,b1)*t1b_old_x(b2,j1)
               T2jb(j1,b1) += T3jb(j1,b1)
            ENDDO b2
            DO j2
               GET Gmi_b(j1,j2)
               GET t1b_old_x(b1,j2)
               T3jb(j1,b1)  = Gmi_b(j1,j2)*t1b_old_x(b1,j2)
               T2jb(j1,b1) -= T3jb(j1,b1)
            ENDDO j2
            Tjb(j,b) = Tbjbj(b,j1,b1,j)*T2jb(j1,b1)
            PUT l1b_old_x(j,b) += Tjb(j,b)
      ENDPARDO b, b1, j, j1
#...............................................................
      PARDO b, a, j, i
            REQUEST Vpiqj(a,i,b,j) j
            T2ia(i,a) = 0.0
            DO a1
               GET Gae_a(a1,a)
               GET t1a_old_x(a1,i)
               T3ia(i,a)  = Gae_a(a1,a)*t1a_old_x(a1,i)
               T2ia(i,a) += T3ia(i,a)
            ENDDO a1
            DO i1
               GET Gmi_a(i,i1)
               GET t1a_old_x(a,i1)
               T3ia(i,a)  = Gmi_a(i,i1)*t1a_old_x(a,i1)
               T2ia(i,a) -= T3ia(i,a)
            ENDDO i1
            Tjb(j,b) = Vpiqj(a,i,b,j)*T2ia(i,a)
            PUT l1b_old_x(j,b) += Tjb(j,b)
      ENDPARDO b, a, j, i

      ENDPROC L1BOLD_X
