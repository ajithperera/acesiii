# Compute contributions to L2new_ab 
            DO i1
            DO a1
               REQUEST L2old_aa(i,a,i1,a1) a1 

               T1iajb(i1,a1,j,b)            = L2old_aa(i,a,i1,a1)*W1MEBJ_BA_x(j,b,a,i)  
               PREPARE L2old_ab_x(i1,a1,j,b) += T1iajb(i1,a1,j,b)
            ENDDO a1
            ENDDO i1
      ENDPARDO j, b, a, i 
