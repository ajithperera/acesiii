        PROC get_HBx_Dipole
#       -------------------
         CREATE HBx
         execute sip_barrier

         PARDO mu, nu
              IF X_COMP == 9.0
                 EXECUTE return_x Txx
              ENDIF

              IF Y_COMP == 9.0
                 EXECUTE return_y Txx
              ENDIF

              IF Z_COMP == 9.0
                 EXECUTE return_z Txx
              ENDIF

              PUT  HBx(mu,nu) += Txx(mu,nu)
         ENDPARDO mu,nu

         execute sip_barrier
        ENDPROC get_HBx_Dipole
#       ----------------------

        PROC get_HBx_Angmom
#       -------------------
         CREATE HBx
         execute sip_barrier

         PARDO mu, nu

             IF X_COMP == 9.0
                EXECUTE compute_angmom_xbatch Txx(mu,nu)
             ENDIF

             IF Y_COMP == 9.0
                EXECUTE compute_angmom_ybatch Txx(mu,nu)
             ENDIF

             IF Z_COMP == 9.0
                EXECUTE compute_angmom_zbatch Txx(mu,nu)
             ENDIF

             PUT  HBx(mu,nu) += Txx(mu,nu)
         ENDPARDO mu,nu

         execute sip_barrier

        ENDPROC get_HBx_Angmom
#       ----------------------

        PROC get_HBx_Spin_orbit
#       ----------------------
         CREATE HBx
         execute sip_barrier

         PARDO mu, nu

             IF X_COMP == 9.0
                EXECUTE compute_1soi_xbatch Txx(mu,nu)
             ENDIF

             IF Y_COMP == 9.0
                EXECUTE compute_1soi_ybatch Txx(mu,nu)
             ENDIF

             IF Z_COMP == 9.0
                EXECUTE compute_1soi_zbatch Txx(mu,nu)
             ENDIF
             PUT  HBx(mu,nu) = Txx(mu,nu)

              PUT  HBx(mu,nu) += Txx(mu,nu)
         ENDPARDO mu,nu

         execute sip_barrier

        ENDPROC get_HBx_Spin_orbit
#       --------------------------

         PROC TRANS2_MO
#        --------------
         CREATE HBa_ae_x        #PV alpha virt-virt block
         CREATE HBa_me_x        #PV alpha Virt-occ block
         CREATE HBa_mi_x        #PV alpha occ-occ  block
         CREATE HBb_ae_x        #PV beta  virt-virt block
         CREATE HBb_me_x        #PV beta  virt-virt block
         CREATE HBb_mi_x        #PV beta  virt-virt block
         execute sip_barrier

          PARDO mu,nu
            get HBx(mu,nu)
            do i
                Tix(i,nu) = HBx(mu,nu)*CA(mu,i)
                do i1
                   Tii(i,i1) = Tix(i,nu)*CA(nu,i1)
                   PUT HBa_mi_x(i,i1) += Tii(i,i1)
                enddo i1
           enddo i
        ENDPARDO mu,nu
#.................................
        PARDO mu,nu
            get HBx(mu,nu)
            do i
                Tix(i,nu) = HBx(mu,nu)*CA(mu,i)
                do a
                   Tia(i,a)  = Tix(i,nu)*CA(nu,a)
                    PUT HBa_me_x(i,a) += Tia(i,a)
                enddo a
            enddo i
        ENDPARDO mu,nu
#..............................................
        PARDO mu,nu
            get HBx(mu,nu)
            do a
               Txa(mu,a) = HBx(mu,nu)*ca(nu,a)
                do a1
                   Taa(a1,a)  = Txa(mu,a)*CA(mu,a1)
                   PUT HBa_ae_x(a1,a) += Taa(a1,a)
               enddo a1
            enddo a
        ENDPARDO mu,nu
#.................BETA PARTS.............................

       IF BETA_IS_MINUS == 100.0

         PARDO mu,nu
            get HBx(mu,nu)
            do j
                Tjx(j,nu) = HBx(mu,nu)*CB(mu,j)
                do j1
                   Tjj(j,j1) = Tjx(j,nu)*CB(nu,j1)
                   Tjj(j,j1) *= -1.0
                   PUT HBb_mi_x(j,j1) += Tjj(j,j1)
                enddo j1
            enddo j
         ENDPARDO mu,nu
#..................................
          PARDO mu,nu
            get HBx(mu,nu)
            do j
                Tjx(j,nu) = HBx(mu,nu)*CB(mu,j)
                do b
                   Tjb(j,b)  = Tjx(j,nu)*CB(nu,b)
                   Tjb(j,b)  *= -1.0
                   PUT HBb_me_x(j,b) += Tjb(j,b)
                enddo b
             enddo j
         ENDPARDO mu,nu
#.....................................
          PARDO mu,nu
            get HBx(mu,nu)
             do b
                Txb(mu,b) = HBx(mu,nu)*CB(nu,b)
               do b1
                   Tbb(b1,b)  = Txb(mu,b)*CB(mu,b1)
                   Tbb(b1,b)  *= -1.0
                   PUT HBb_ae_x(b1,b) += Tbb(b1,b)
                enddo b1
             enddo b
         ENDPARDO mu,nu

       ELSE 

         PARDO mu,nu
            get HBx(mu,nu)
            do j
                Tjx(j,nu) = HBx(mu,nu)*CB(mu,j)
                do j1
                   Tjj(j,j1) = Tjx(j,nu)*CB(nu,j1)
                   Tjj(j,j1) *= -1.0
                   PUT HBb_mi_x(j,j1) += Tjj(j,j1)
                enddo j1
            enddo j
         ENDPARDO mu,nu
#..................................
          PARDO mu,nu
            get HBx(mu,nu)
            do j
                Tjx(j,nu) = HBx(mu,nu)*CB(mu,j)
                do b
                   Tjb(j,b)  = Tjx(j,nu)*CB(nu,b)
                   PUT HBb_me_x(j,b) += Tjb(j,b)
                enddo b
             enddo j
         ENDPARDO mu,nu
#.....................................
          PARDO mu,nu
            get HBx(mu,nu)
             do b
                Txb(mu,b) = HBx(mu,nu)*CB(nu,b)
               do b1
                   Tbb(b1,b)  = Txb(mu,b)*CB(mu,b1)
                   PUT HBb_ae_x(b1,b) += Tbb(b1,b)
                enddo b1
             enddo b
         ENDPARDO mu,nu
       ENDIF

         execute sip_barrier

      ENDPROC Trans2_MO
