                  REQUEST t2old_bb_x(b1,j,b,j1) j1
                  REQUEST Vpiqj(a,i,b1,j1)    j1
                  GET t1b_old(b1,j)
                  GET t1b_old_x(b1,j)
                  tbj(b,j1)          = t1b_old_x(b,j1)
                  T2bjbj(b1,j,b,j1)  = t1b_old(b1,j)^tbj(b,j1)
                  T2bjbj(b1,j,b,j1) += T2old_bb_x(b1,j,b,j1)

                  tbj(b,j1)          = t1b_old(b,j1)
                  T1bjbj(b1,j,b,j1)  = t1b_old_x(b1,j)^tbj(b,j1)
                  T2bjbj(b1,j,b,j1) += T1bjbj(b1,j,b,j1)                

                  T1iabj(i,a,b,j)    = T2bjbj(b1,j,b,j1)*Vpiqj(a,i,b1,j1)
                  Tiabj(i,a,b,j)    -= T1iabj(i,a,b,j)
               ENDDO b1
            ENDDO j1
            DO a1
            DO i1
               REQUEST T2old_ab_x(a1,i1,b,j) j
               REQUEST VSpipi(a1,i1,a,i)   i
               T1iabj(i,a,b,j) = T2old_ab_x(a1,i1,b,j)*VSpipi(a1,i1,a,i)
               Tiabj(i,a,b,j) += T1iabj(i,a,b,j)
            ENDDO i1
            ENDDO a1
            PREPARE W1MEBJ_AB_x(i,a,b,j) = Tiabj(i,a,b,j)
      ENDPARDO j, i, a, b
      ENDPROC W1MEBJAB_S_x
#     --------------------

      PROC W1MEBJAB 
#     -------------
      PARDO i, a, b, j 
            REQUEST W1MEBJ_AB_X(i,a,b,j) a 
# Compute contribution to l1a_new 
            GET l1b_old(j,b)
            T1ia(i,a)         = W1MEBJ_AB_X(i,a,b,j)*l1b_old(j,b)
            PUT l1a_old_x(i,a) += T1ia(i,a) 
# Compute contributions to L2new_aa 
            DO i1  
            DO a1  
               REQUEST L2old_ab(i1,a1,j,b) b 

               T1iaia(i1,a1,i,a)  = L2old_ab(i1,a1,j,b)*W1MEBJ_AB_X(i,a,b,j)
               T2iaia(i1,a,i,a1)  = T1iaia(i1,a1,i,a)
               T2iaia(i1,a,i,a1) *= -1.0  
               T3iaia(i,a1,i1,a)  = T1iaia(i1,a1,i,a)
               T3iaia(i,a1,i1,a) *= -1.0  
               T4iaia(i,a,i1,a1)  = T1iaia(i1,a1,i,a)

               PREPARE L2old_aa_x(i1,a1,i,a) += T1iaia(i1,a1,i,a) 
               PREPARE L2old_aa_x(i1,a,i,a1) += T2iaia(i1,a,i,a1) 
               PREPARE L2old_aa_x(i,a1,i1,a) += T3iaia(i,a1,i1,a) 
               PREPARE L2old_aa_x(i,a,i1,a1) += T4iaia(i,a,i1,a1) 

            ENDDO a1  
            ENDDO i1  
# Compute contributions to L2new_ab 
            DO j1
            DO b1
               REQUEST L2old_bb(j,b,j1,b1) b1 

               T1iajb(i,a,j1,b1)            = L2old_bb(j,b,j1,b1)*W1MEBJ_AB_x(i,a,b,j)
               PREPARE L2old_ab_x(i,a,j1,b1) += T1iajb(i,a,j1,b1)
            ENDDO b1
            ENDDO j1
      ENDPARDO i, a, b, j 
      ENDPROC W1MEBJAB 
