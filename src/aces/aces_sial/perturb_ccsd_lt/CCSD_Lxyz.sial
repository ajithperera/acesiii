                  T1bjbj(b2,j,b,j2)  += t2old_bb_x(b2,j,b,j2)

                  tbj(b2,j)           = t1b_old(b2,j)
                  T2bjbj(b2,j,b,j2)   = t1b_old_x(b,j2)^tbj(b2,j)
                  T1bjbj(b2,j,b,j2)  += T2bjbj(b2,j,b,j2)               

                  T1jbbj(j1,b1,b,j)   = T1bjbj(b2,j,b,j2)*VSqjqj(b2,j2,b1,j1)
