               T3iaia(i,a1,i1,a)  = T1iaia(i1,a1,i,a)
               T3iaia(i,a1,i1,a) *= -1.0
               T4iaia(i,a,i1,a1)  = T1iaia(i1,a1,i,a)

               PREPARE L2new_aa_x(i1,a1,i,a) += T1iaia(i1,a1,i,a)
               PREPARE L2new_aa_x(i1,a,i,a1) += T2iaia(i1,a,i,a1)
               PREPARE L2new_aa_x(i,a1,i1,a) += T3iaia(i,a1,i1,a)
               PREPARE L2new_aa_x(i,a,i1,a1) += T4iaia(i,a,i1,a1)
            ENDDO a1
            ENDDO i1
# Compute contributions to L2new_ab 
            DO j1
            DO b1
               REQUEST L2old_bb_x(j,b,j1,b1) b1
               T1iajb(i,a,j1,b1)            = L2old_bb_x(j,b,j1,b1)*W1MEBJ_AB(i,a,b,j)
               PREPARE L2new_ab_x(i,a,j1,b1) += T1iajb(i,a,j1,b1)
            ENDDO b1
            ENDDO j1
      ENDPARDO i, a, b, j

