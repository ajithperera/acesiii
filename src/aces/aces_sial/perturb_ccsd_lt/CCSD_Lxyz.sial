      PARDO j1, b1, b, j 
            REQUEST W1MEBJ_BB_x(j1,b1,b,j) b 
# Compute contribution to L1b_new  
            GET l1b_old(j,b)
            Tjb(j1,b1)          = W1MEBJ_BB_X(j1,b1,b,j)*l1b_old(j,b)
