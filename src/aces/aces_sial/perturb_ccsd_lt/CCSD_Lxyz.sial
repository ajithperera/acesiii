      PARDO i, a, b, j
            REQUEST W1MEBJ_AB(i,a,b,j) a
# Compute contribution to l1a_new 
            GET l1b_old_x(j,b)
            T1ia(i,a)         = W1MEBJ_AB(i,a,b,j)*l1b_old_x(j,b)
