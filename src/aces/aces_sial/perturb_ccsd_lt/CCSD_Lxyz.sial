            PREPARE T2old_bb_X(B,J,B1,J1)= Tbjbj(B,J,B1,J1)
      ENDPARDO B, B1, J, J1

      PARDO A, B, I, J
            REQUEST T2ab_Y(A,I,B,J) J
            Taibj(A,I,B,J) = T2ab_Y(A,I,B,J)
            PREPARE T2old_ab_X(A,I,B,J) = Taibj(A,I,B,J)
