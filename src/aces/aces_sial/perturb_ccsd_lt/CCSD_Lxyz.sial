#           T1jbai(j,b,a,i2)  = Xjjai(j1,j,a1,i2)*Vaabj(a,a1,b,j1)*t1a_old(a,i2) 
            REQUEST Vaabj(a,a1,b,j1) j1
            DO j
            DO i2
               GET                 Xjjai(j1,j,a1,i2)
               GET                 t1a_old_x(a,i2)

               Tjija(j,i2,j1,a1) = Xjjai(j1,j,a1,i2)
               Tjaba(j1,a1,b,a)  = Vaabj(a,a1,b,j1)

               Tjiba(j,i2,b,a)   = Tjija(j,i2,j1,a1)*Tjaba(j1,a1,b,a)
               Tjbai(j,b,a,i2)   = Tjiba(j,i2,b,a)

               tjb(j,b)          = Tjbai(j,b,a,i2)*t1a_old_x(a,i2)
               tjb(j,b)         *= -1.0
               PUT l1b_old_x(j,b) += tjb(j,b)
            ENDDO i2
            ENDDO j
      ENDPARDO a, b, j1, a1
      execute sip_barrier
      delete Xjbii
      delete Xjjai
# 'Fix' the final abomination 
# --------------------------- 
      create Xjjii
      create Xjbii
      execute sip_barrier
      PARDO j1, i1, b1, a1
            REQUEST Tau_ab(a1,i1,b1,j1) j1
            DO j
            DO i2
               REQUEST                  L2old_ab(i2,a1,j,b1) b1
               Tjjii(j,j1,i2,i1)      = L2old_ab(i2,a1,j,b1)*Tau_ab(a1,i1,b1,j1)
               PUT Xjjii(j,j1,i2,i1) += Tjjii(j,j1,i2,i1)
            ENDDO i2
            ENDDO j
      ENDPARDO j1, i1, b1, a1
#..........................
      PARDO j1, i1, b, a
            REQUEST Vpiqj(a,i1,b,j1) j1
            DO i2
               GET                      t1a_old_x(a,i2)
               Tjbii(j1,b,i1,i2)      = Vpiqj(a,i1,b,j1)*t1a_old_x(a,i2)
               PUT Xjbii(j1,b,i1,i2) += Tjbii(j1,b,i1,i2)
            ENDDO i2
      ENDPARDO j1, i1, b, a
      execute sip_barrier
#....................................
      PARDO b, i1, i2, j1
            GET Xjbii(j1,b,i1,i2)
            DO j
               GET                 Xjjii(j,j1,i2,i1)
               tjb(j,b)          = Xjjii(j,j1,i2,i1)*Xjbii(j1,b,i1,i2)
               PUT l1b_old_x(j,b) += tjb(j,b)
            ENDDO j
      ENDPARDO b, i1, i2, j1
      execute sip_barrier
      delete Xjjii
      delete Xjbii
      ENDPROC L1WABCDbX
