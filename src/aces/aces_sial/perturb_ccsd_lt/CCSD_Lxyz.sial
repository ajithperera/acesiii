           esumb += etemp
     ENDPARDO b, b1, j, j1
     ecorrab = 0.0
     esumab  = 0.0
     PARDO a, b, i, j
           REQUEST Vpiqj(a,i,b,j) j
           REQUEST L2old_ab_x(i,a,j,b) b

