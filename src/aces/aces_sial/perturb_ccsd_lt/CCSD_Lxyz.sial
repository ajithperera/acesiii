               GET                  t1a_old_x(a,i2)

               T1iaai(i1,a1,a,i)  = VSpipi(a1,i1,i,i2)*t1a_old_x(a,i2)
               Tiaai(i1,a1,a,i)  -= T1iaai(i1,a1,a,i)
