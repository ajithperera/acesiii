     PROC L1ANEW_X
       PARDO i, a, a1
            GET                 l1a_old_x(i,a1)
            GET                 F1ae_a(a1,a)
            Tia(i,a)          = l1a_old_x(i,a1)*F1ae_a(a1,a)
            PUT l1a_new_x(i,a) += Tia(i,a)
       ENDPARDO i, a, a1
#..................................................
       PARDO i, a, a1, a2
            REQUEST             VSaaai(a2,a1,a,i) i
            GET                 Gae_a_x(a1,a2)
            Tia(i,a)          = VSaaai(a2,a1,a,i)*Gae_a_x(a1,a2)
            Tia(i,a)         *= -1.0
            PUT l1a_new_x(i,a) += Tia(i,a)
        ENDPARDO i, a, a1, a2
#....................................................
       PARDO i, a, i1
            GET                 l1a_old_x(i1,a)
            GET                 F1mi_a(i,i1)
            Tia(i,a)          = l1a_old_x(i1,a)*F1mi_a(i,i1)
            Tia(i,a)         *= -1.0
            PUT l1a_new_x(i,a) += Tia(i,a)
        ENDPARDO i, a, i1
#.....................................................
       PARDO a, i, i1, i2
            REQUEST             VSpipi(a,i,i1,i2) i2
            GET                 Gmi_a_x(i2,i1)
            Tia(i,a)          = VSpipi(a,i,i1,i2)*Gmi_a_x(i2,i1)
            Tia(i,a)         *= -1.0
            PUT l1a_new_x(i,a) += Tia(i,a)
      ENDPARDO a, i, i1, i2
      PARDO a, i, j, j1
            REQUEST             Vpiqj(a,i,j,j1) j1
            GET                 Gmi_b_x(j1,j)
            Tia(i,a)          = Vpiqj(a,i,j,j1)*Gmi_b_x(j1,j)
            Tia(i,a)         *= -1.0
            PUT l1a_new_x(i,a) += Tia(i,a)
      ENDPARDO a, i, j, j1
#......................................................
     PARDO a, i, b, b1
            REQUEST             Vbbai(b1,b,a,i) i
            GET                 Gae_b_x(b,b1)
            Tia(i,a)          = Vbbai(b1,b,a,i)*Gae_b_x(b,b1)
            Tia(i,a)         *= -1.0
            PUT l1a_new_x(i,a) += Tia(i,a)
      ENDPARDO a, i, b, b1
#...................................................
      PARDO i1, a1, i2
            allocate Liaia(i1,*,i2,a1)
            DO a
               REQUEST             L2old_aa_x(i1,a,i2,a1)  a1
               Liaia(i1,a,i2,a1) = L2old_aa_x(i1,a,i2,a1)
            ENDDO a
            DO i
               REQUEST W1imen_aa(i,i1,a1,i2) i2
               DO a
                  Tia(i,a)          = W1imen_aa(i,i1,a1,i2)*Liaia(i1,a,i2,a1)
                  Tia(i,a)         *= -0.5
                  PUT l1a_new_x(i,a) += Tia(i,a)
               ENDDO a
            ENDDO i
            deallocate Liaia(i1,*,i2,a1)
      ENDPARDO i1, a1, i2
#..............................................
      PARDO j, b, i1
            allocate Liajb(i1,*,j,b)
            DO a
               REQUEST           L2old_ab_x(i1,a,j,b)  b
               Liajb(i1,a,j,b) = L2old_ab_x(i1,a,j,b)
            ENDDO a
            DO i
               REQUEST W1imen_ab(i,i1,b,j) b
               DO a
                  Tia(i,a)          = W1imen_ab(i,i1,b,j)*Liajb(i1,a,j,b)
                  Tia(i,a)         *= -1.0
                  PUT l1a_new_x(i,a) += Tia(i,a)
               ENDDO a
            ENDDO i
            deallocate Liajb(i1,*,j,b)
      ENDPARDO j, b, i1
#......................................
       PARDO a, i, a1, i1
            REQUEST             VSpipi(a,i,a1,i1) i1
            Taiai(a,i,a1,i1)  = VSpipi(a,i,a1,i1)
            T2ia(i1,a1) = 0.0
            DO a2
               GET Gae_a_x(a2,a1)
               GET t1a_old(a2,i1)

               T3ia(i1,a1)  = Gae_a_x(a2,a1)*t1a_old(a2,i1)
               T2ia(i1,a1) += T3ia(i1,a1)
            ENDDO a2
            DO i2
               GET Gmi_a_x(i1,i2)
               GET t1a_old(a1,i2)

               T3ia(i1,a1)  = Gmi_a_x(i1,i2)*t1a_old(a1,i2)
               T2ia(i1,a1) -= T3ia(i1,a1)
            ENDDO i2
            Tia(i,a)          = Taiai(a,i,a1,i1)*T2ia(i1,a1)
            PUT l1a_new_x(i,a) += Tia(i,a)
      ENDPARDO a, i, a1, i1
#................................................................
      PARDO a, b, i, j
            REQUEST Vpiqj(a,i,b,j) j
            T2jb(j,b) = 0.0
            DO b1
               GET Gae_b_x(b1,b)
               GET t1b_old(b1,j)

               T3jb(j,b)  = Gae_b_x(b1,b)*t1b_old(b1,j)
               T2jb(j,b) += T3jb(j,b)
            ENDDO b1
            DO j1
               GET Gmi_b_x(j,j1)
               GET t1b_old(b,j1)

               T3jb(j,b)  = Gmi_b_x(j,j1)*t1b_old(b,j1)
               T2jb(j,b) -= T3jb(j,b)
            ENDDO j1
            Tia(i,a)          = Vpiqj(a,i,b,j)*T2jb(j,b)
            PUT l1a_new_x(i,a) += Tia(i,a)
      ENDPARDO a, b, i, j
     ENDPROC L1ANEW_X
#______________________________________________________
      PROC L1AOLD_X
#     --------------
      PARDO a, i  
            GET                   Fme_a_x(i,a)
            Tia(i,a)            = Fme_a_x(i,a) 
            PUT l1a_old_x(i,a) += Tia(i,a)
      ENDPARDO a, i
#......................
      PARDO i, a, a1  
            GET                 l1a_old(i,a1)
            GET                 F1ae_a_x(a1,a)
            Tia(i,a)          = l1a_old(i,a1)*F1ae_a_x(a1,a)
            PUT l1a_old_x(i,a) += Tia(i,a)
      ENDPARDO i, a, a1  
#.................................................................
#pv      PARDO i, a, a1, a2  
#pv            REQUEST             VSaaai(a2,a1,a,i) i
#pv            GET                 Gae_a(a1,a2)
#Pv            Tia(i,a)          = VSaaai(a2,a1,a,i)*Gae_a(a1,a2)
#pv            Tia(i,a)         *= -1.0  
#pv            PUT l1a_new(i,a) += Tia(i,a)
#pv      ENDPARDO i, a, a1, a2  
#................................................................
      PARDO i, a, i1 
            GET                 l1a_old(i1,a)
            GET                 F1mi_a_x(i,i1)
            Tia(i,a)          = l1a_old(i1,a)*F1mi_a_x(i,i1)
            Tia(i,a)         *= -1.0  
            PUT l1a_old_x(i,a) += Tia(i,a)
      ENDPARDO i, a, i1  
#...............................................................
#pv      PARDO a, i, i1, i2   
#pv            REQUEST             VSpipi(a,i,i1,i2) i2  
#pv            GET                 Gmi_a(i2,i1)
#pv            Tia(i,a)          = VSpipi(a,i,i1,i2)*Gmi_a(i2,i1)
#pv            Tia(i,a)         *= -1.0  
#pv            PUT l1a_new(i,a) += Tia(i,a)
#pv      ENDPARDO a, i, i1, i2 
#pv      PARDO a, i, j, j1   
#pv            REQUEST             Vpiqj(a,i,j,j1) j1  
#pv            GET                 Gmi_b(j1,j)
#pv            Tia(i,a)          = Vpiqj(a,i,j,j1)*Gmi_b(j1,j)
#pv            Tia(i,a)         *= -1.0  
#pv            PUT l1a_new(i,a) += Tia(i,a)
#pv      ENDPARDO a, i, j, j1 
#pv      PARDO a, i, b, b1  
#pv            REQUEST             Vbbai(b1,b,a,i) i
#pv            GET                 Gae_b(b,b1)
#pv            Tia(i,a)          = Vbbai(b1,b,a,i)*Gae_b(b,b1)
#pv            Tia(i,a)         *= -1.0  
#pv            PUT l1a_new(i,a) += Tia(i,a)
#pv      ENDPARDO a, i, b, b1 
#...............................................................
      PARDO i1, a1, i2  
            allocate Liaia(i1,*,i2,a1) 
            DO a 
               REQUEST             L2old_aa(i1,a,i2,a1)  a1
               Liaia(i1,a,i2,a1) = L2old_aa(i1,a,i2,a1) 
            ENDDO a 
            DO i
               REQUEST W1imen_aa_x(i,i1,a1,i2) i2 
               DO a
                  Tia(i,a)          = W1imen_aa_x(i,i1,a1,i2)*Liaia(i1,a,i2,a1)
                  Tia(i,a)         *= -0.5
                  PUT l1a_old_x(i,a) += Tia(i,a)
               ENDDO a
            ENDDO i
            deallocate Liaia(i1,*,i2,a1) 
      ENDPARDO i1, a1, i2  
#........................
      PARDO j, b, i1  
            allocate Liajb(i1,*,j,b) 
            DO a 
               REQUEST           L2old_ab(i1,a,j,b)  b 
               Liajb(i1,a,j,b) = L2old_ab(i1,a,j,b) 
            ENDDO a 
            DO i
               REQUEST W1imen_ab_x(i,i1,b,j) b 
               DO a
                  Tia(i,a)          = W1imen_ab_x(i,i1,b,j)*Liajb(i1,a,j,b) 
                  Tia(i,a)         *= -1.0
                  PUT l1a_old_x(i,a) += Tia(i,a)
               ENDDO a
            ENDDO i
            deallocate Liajb(i1,*,j,b) 
      ENDPARDO j, b, i1  
#.............................
      PARDO a, i, a1, i1
            REQUEST             VSpipi(a,i,a1,i1) i1  
            Taiai(a,i,a1,i1)  = VSpipi(a,i,a1,i1)
            T2ia(i1,a1) = 0.0
            DO a2
               GET Gae_a(a2,a1)
               GET t1a_old_x(a2,i1)
               T3ia(i1,a1)  = Gae_a(a2,a1)*t1a_old_x(a2,i1)
               T2ia(i1,a1) += T3ia(i1,a1)
            ENDDO a2
            DO i2
               GET Gmi_a(i1,i2)
               GET t1a_old_x(a1,i2)
               T3ia(i1,a1)  = Gmi_a(i1,i2)*t1a_old_x(a1,i2)
               T2ia(i1,a1) -= T3ia(i1,a1)
            ENDDO i2
            Tia(i,a)          = Taiai(a,i,a1,i1)*T2ia(i1,a1)
            PUT l1a_old_x(i,a) += Tia(i,a)
      ENDPARDO a, i, a1, i1
#......................................
      PARDO a, b, i, j
            REQUEST Vpiqj(a,i,b,j) j
            T2jb(j,b) = 0.0
            DO b1
               GET Gae_b(b1,b)
               GET t1b_old_x(b1,j)
               T3jb(j,b)  = Gae_b(b1,b)*t1b_old_x(b1,j)
               T2jb(j,b) += T3jb(j,b)
            ENDDO b1
            DO j1
               GET Gmi_b(j,j1)
               GET t1b_old_x(b,j1)
               T3jb(j,b)  = Gmi_b(j,j1)*t1b_old_x(b,j1)
               T2jb(j,b) -= T3jb(j,b)
            ENDDO j1
            Tia(i,a)          = Vpiqj(a,i,b,j)*T2jb(j,b)
            PUT l1a_old_x(i,a) += Tia(i,a)
      ENDPARDO a, b, i, j

      ENDPROC L1AOLD_X
#     ----------------
#__________________________________________________________________
      PROC L1BNEW_X
#     -------------
        PARDO b, j, j1                 
            GET                 l1b_old_x(j1,b)
            GET                 F1mi_b(j,j1)
            Tjb(j,b)          = l1b_old_x(j1,b)*F1mi_b(j,j1)
            Tjb(j,b)         *= -1.0
            PUT l1b_new_x(j,b) += Tjb(j,b)
      ENDPARDO b, j, j1
#............................
      PARDO b, j, j1, j2
            REQUEST             VSqjqj(b,j,j1,j2) j2
            GET                 Gmi_b_x(j2,j1)
            Tjb(j,b)          = VSqjqj(b,j,j1,j2)*Gmi_b_x(j2,j1)
            Tjb(j,b)         *= -1.0
            PUT l1b_new_x(j,b) += Tjb(j,b)
      ENDPARDO b, j, j1, j2
#...........................
       PARDO b, b1, j1, j2
            REQUEST L2old_bb_x(j1,b,j2,b1)  b1
            DO j
               REQUEST             W1imen_bb(j,j1,b1,j2) j2
               Tjb(j,b)          = W1imen_bb(j,j1,b1,j2)*L2old_bb_x(j1,b,j2,b1)
               Tjb(j,b)         *= -0.5
               PUT l1b_new_x(j,b) += Tjb(j,b)
            ENDDO j
      ENDPARDO b, b1, j1, j2
#..............................
      PARDO j, b, b1
            GET                 l1b_old_x(j,b1)
            GET                 F1ae_b(b1,b)
            Tjb(j,b)          = l1b_old_x(j,b1)*F1ae_b(b1,b)
            PUT l1b_new_x(j,b) += Tjb(j,b)
      ENDPARDO j, b, b1
#................................
      PARDO b, j, b1, b2
            REQUEST             VSbbbj(b2,b1,b,j) j
            GET                 Gae_b_x(b1,b2)
            Tjb(j,b)          = VSbbbj(b2,b1,b,j)*Gae_b_x(b1,b2)
            Tjb(j,b)         *= -1.0
            PUT l1b_new_x(j,b) += Tjb(j,b)
      ENDPARDO b, j, b1, b2
#...................................
      PARDO b, j, i, i1
            REQUEST             Vpiqj(i,i1,b,j) j
            GET                 Gmi_a_x(i1,i)
            Tjb(j,b)          = Vpiqj(i,i1,b,j)*Gmi_a_x(i1,i)
            Tjb(j,b)         *= -1.0
            PUT l1b_new_x(j,b) += Tjb(j,b)
      ENDPARDO b, j, i, i1
#........................................
      PARDO b, j1, a, i
            REQUEST L2old_ab_x(i,a,j1,b) b
            DO j
               REQUEST             W1imen_ba(j,j1,a,i) i
               Tjb(j,b)          = W1imen_ba(j,j1,a,i)*L2old_ab_x(i,a,j1,b)
               Tjb(j,b)         *= -1.0
               PUT l1b_new_x(j,b) += Tjb(j,b)
            ENDDO j
      ENDPARDO b, j1, a, i
#..............................................
      PARDO b, j, a, a1
            REQUEST             Vaabj(a1,a,b,j) j
            GET                 Gae_a_x(a,a1)
            Tjb(j,b)          = Vaabj(a1,a,b,j)*Gae_a_x(a,a1)
            Tjb(j,b)         *= -1.0
            PUT l1b_new_x(j,b) += Tjb(j,b)
      ENDPARDO b, j, a, a1
#..................................................
      PARDO b, b1, j, j1
            REQUEST VSqjqj(b,j,b1,j1) j1
            Tbjbj(b,j1,b1,j) = VSqjqj(b,j,b1,j1)
            T2jb(j1,b1) = 0.0
            DO b2
               GET Gae_b_x(b2,b1)
               GET t1b_old(b2,j1)

               T3jb(j1,b1)  = Gae_b_x(b2,b1)*t1b_old(b2,j1)
               T2jb(j1,b1) += T3jb(j1,b1)
            ENDDO b2
            DO j2
               GET Gmi_b_x(j1,j2)
               GET t1b_old(b1,j2)

               T3jb(j1,b1)  = Gmi_b_x(j1,j2)*t1b_old(b1,j2)
               T2jb(j1,b1) -= T3jb(j1,b1)
            ENDDO j2
            Tjb(j,b) = Tbjbj(b,j1,b1,j)*T2jb(j1,b1)
            PUT l1b_new_x(j,b) += Tjb(j,b)
      ENDPARDO b, b1, j, j1
#.........................................................
      PARDO b, a, j, i
            REQUEST Vpiqj(a,i,b,j) j
            T2ia(i,a) = 0.0
            DO a1
               GET Gae_a_x(a1,a)
               GET t1a_old(a1,i)

               T3ia(i,a)  = Gae_a_x(a1,a)*t1a_old(a1,i)
               T2ia(i,a) += T3ia(i,a)
            ENDDO a1
            DO i1
               GET Gmi_a_x(i,i1)
               GET t1a_old(a,i1)

               T3ia(i,a)  = Gmi_a_x(i,i1)*t1a_old(a,i1)
               T2ia(i,a) -= T3ia(i,a)
            ENDDO i1
            Tjb(j,b) = Vpiqj(a,i,b,j)*T2ia(i,a)
            PUT l1b_new_x(j,b) += Tjb(j,b)
      ENDPARDO b, a, j, i

      ENDPROC L1BNEW_X
