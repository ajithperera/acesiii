           t2jbjb(j,b,j1,b1)            = D1jbjb(j,b,j1,b1)  
           t2jbjb(j,b,j1,b1)           += e2jbjb(j,b,j1,b1)
           t2jbjb(j,b,j1,b1)           *= c2
           tjbjb(j,b,j1,b1)            += t2jbjb(j,b,j1,b1)
#
           t2jbjb(j,b,j1,b1)            = D2jbjb(j,b,j1,b1)  
           t2jbjb(j,b,j1,b1)           += e3jbjb(j,b,j1,b1)
           t2jbjb(j,b,j1,b1)           *= c3
           tjbjb(j,b,j1,b1)            += t2jbjb(j,b,j1,b1)
