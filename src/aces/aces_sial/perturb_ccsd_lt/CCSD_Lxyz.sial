               T1jbjb(j2,b2,j1,b1)  = L2old_bb(j2,b2,j,b)*W1MEBJ_BB_x(j1,b1,b,j)
               T2jbjb(j2,b1,j1,b2)  = T1jbjb(j2,b2,j1,b1)
               T2jbjb(j2,b1,j1,b2) *= -1.0  
               T3jbjb(j1,b2,j2,b1)  = T1jbjb(j2,b2,j1,b1)
               T3jbjb(j1,b2,j2,b1) *= -1.0  
               T4jbjb(j1,b1,j2,b2)  = T1jbjb(j2,b2,j1,b1)

               PREPARE L2old_bb_x(j2,b2,j1,b1) += T1jbjb(j2,b2,j1,b1) 
               PREPARE L2old_bb_x(j2,b1,j1,b2) += T2jbjb(j2,b1,j1,b2) 
               PREPARE L2old_bb_x(j1,b2,j2,b1) += T3jbjb(j1,b2,j2,b1)  
               PREPARE L2old_bb_x(j1,b1,j2,b2) += T4jbjb(j1,b1,j2,b2)  

            ENDDO b2
            ENDDO j2
# Compute contributions to L2new_ab 
            DO i
