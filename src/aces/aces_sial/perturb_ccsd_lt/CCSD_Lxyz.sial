            Tbj(B,J) = T1b_Y(B,J)
            PUT T1b_old_x(B,J) = Tbj(B,J)
      ENDPARDO J,B 

      PARDO A, A1, I, I1
            REQUEST T2aa_Y(A,I,A1,I1) I1
            Taiai(A,I,A1,I1) = T2aa_Y(A,I,A1,I1)
            PREPARE T2old_aa_X(A,I,A1,I1) = Taiai(A,I,A1,I1)
