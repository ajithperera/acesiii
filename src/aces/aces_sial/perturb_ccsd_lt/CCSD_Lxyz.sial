               DO b1
                  Tbb(b,b1)         = Taup_ab(a,i,b,j)*L2aibj(a,i,b1,j)
                  Tbb(b,b1)        *= -1.0
                  PUT F1ae_b(b,b1) += Tbb(b,b1)
               ENDDO b1
            ENDDO b
            deallocate L2aibj(a,i,*,j)
      ENDPARDO i, j, a

      ENDPROC F1AEAB
