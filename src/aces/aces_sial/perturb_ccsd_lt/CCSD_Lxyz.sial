               Tiabj(i,a,b,j2)   = Tijab(i,j2,a,b)

               tia(i,a)          = Tiabj(i,a,b,j2)*t1b_old(b,j2)
               tia(i,a)         *= -1.0
               PUT l1a_old_x(i,a) += tia(i,a)
            ENDDO i
            ENDDO j2  
      ENDPARDO a, b, b1, i1  
      execute sip_barrier
      delete Xiajj
      delete Xiibj
# Fix the final abomination 
# ------------------------- 
#
      create Xiijj 
      create Xiajj 
      execute sip_barrier 
#....................................................................
      PARDO i1, j1, a1, b1  
            REQUEST Tau_ab_x(a1,i1,b1,j1) j1 
            DO i
            DO j2  
               REQUEST                  L2old_ab(i,a1,j2,b1) b1
               Tiijj(i,i1,j2,j1)      = L2old_ab(i,a1,j2,b1)*Tau_ab_x(a1,i1,b1,j1)
               PUT Xiijj(i,i1,j2,j1) += Tiijj(i,i1,j2,j1)
            ENDDO j2  
            ENDDO i
      ENDPARDO i1, j1, a1, b1  
#...................
      PARDO i1, j1, a, b  
            REQUEST Vpiqj(a,i1,b,j1) j1 
            DO j2  
               GET                      t1b_old(b,j2)
               Tiajj(i1,a,j1,j2)      = Vpiqj(a,i1,b,j1)*t1b_old(b,j2)
               PUT Xiajj(i1,a,j1,j2) += Tiajj(i1,a,j1,j2)
            ENDDO j2  
      ENDPARDO i1, j1, a, b  
      execute sip_barrier 
#..........................
      PARDO a, i1, j1, j2
            GET Xiajj(i1,a,j1,j2) 
            DO i
               GET                 Xiijj(i,i1,j2,j1) 
               tia(i,a)          = Xiijj(i,i1,j2,j1)*Xiajj(i1,a,j1,j2)
               PUT l1a_old_x(i,a) += tia(i,a)
            ENDDO i
      ENDPARDO a, i1, j1, j2
      execute sip_barrier 
      delete Xiijj 
      delete Xiajj 
      ENDPROC L1WABCDa 
