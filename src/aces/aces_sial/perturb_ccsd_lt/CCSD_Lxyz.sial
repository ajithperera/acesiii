               REQUEST                  L2old_ab(i,a1,j2,b1) b1
               Tiijj(i,i1,j2,j1)      = L2old_ab(i,a1,j2,b1)*Tau_ab(a1,i1,b1,j1)
               PUT Xiijj(i,i1,j2,j1) += Tiijj(i,i1,j2,j1)
            ENDDO j2
            ENDDO i
      ENDPARDO i1, j1, a1, b1
#...................
      PARDO i1, j1, a, b
            REQUEST Vpiqj(a,i1,b,j1) j1
            DO j2
               GET                      t1b_old_x(b,j2)
               Tiajj(i1,a,j1,j2)      = Vpiqj(a,i1,b,j1)*t1b_old_x(b,j2)
               PUT Xiajj(i1,a,j1,j2) += Tiajj(i1,a,j1,j2)
            ENDDO j2
      ENDPARDO i1, j1, a, b
      execute sip_barrier
#..........................
      PARDO a, i1, j1, j2
            GET Xiajj(i1,a,j1,j2)
            DO i
               GET                 Xiijj(i,i1,j2,j1)
               tia(i,a)          = Xiijj(i,i1,j2,j1)*Xiajj(i1,a,j1,j2)
               PUT l1a_old_x(i,a) += tia(i,a)
            ENDDO i
      ENDPARDO a, i1, j1, j2
      execute sip_barrier
      delete Xiijj
      delete Xiajj
      ENDPROC L1WABCDaX
#___________________________________________________________________________________
      PROC L1WABCDb 
#     First piece
      create Xjjjb
      execute sip_barrier
      PARDO j, j2, b1, b2
            REQUEST                  L2old_bb(j,b1,j2,b2) b2 
            REQUEST                  L2old_bb(j,b2,j2,b1) b2 
            tjbjb(j,b1,j2,b2)      = L2old_bb(j,b1,j2,b2)
            t1jbjb(j,b1,j2,b2)     = L2old_bb(j,b2,j2,b1)
            t1jbjb(j,b1,j2,b2)    -= tjbjb(j,b1,j2,b2)
            DO j3
               GET                      t1b_old_x(b1,j3)
               tjjjb(j,j3,j2,b2)      = t1jbjb(j,b1,j2,b2)*t1b_old_x(b1,j3)
               PUT Xjjjb(j,j3,j2,b2) += tjjjb(j,j3,j2,b2)
            ENDDO j3
      ENDPARDO j, j2, b1, b2
      execute sip_barrier
#............................
      PARDO b, b3, b2, j3  
            REQUEST VSbbbj(b3,b2,b,j3) j3 
            DO j2
            DO j
               GET                 Xjjjb(j,j3,j2,b2)
               GET                 t1b_old(b3,j2)
               Tjbjb(j,b,j2,b3)  = Xjjjb(j,j3,j2,b2)*VSbbbj(b3,b2,b,j3)
               tjb(j,b)          = Tjbjb(j,b,j2,b3)*t1b_old(b3,j2)
               tjb(j,b)         *= 0.5
               PUT l1b_old_x(j,b) += tjb(j,b)
            ENDDO j
            ENDDO j2
      ENDPARDO b, b3, b2, j3
      execute sip_barrier
      delete Xjjjb
# 'Fix' on 9/29/08 
# ---------------- 
      create Xjjjj 
      create Xjjjb 
      execute sip_barrier 
#..................................................................................
      PARDO j3, j4, b1, b2  
            REQUEST Tau_bb_x(b1,j3,b2,j4) j4 
            DO j
            DO j2
               REQUEST                  L2old_bb(j,b1,j2,b2) b2 
               Tjjjj(j,j3,j2,j4)      = L2old_bb(j,b1,j2,b2)*Tau_bb_x(b1,j3,b2,j4)
               PUT Xjjjj(j,j3,j2,j4) += Tjjjj(j,j3,j2,j4)
            ENDDO j2
            ENDDO j
      ENDPARDO j3, j4, b1, b2 
#......................................
      PARDO j3, j4, b, b3 
            REQUEST VSqjqj(b,j3,b3,j4) j4 
            DO j2  
               GET                      t1b_old(b3,j2)
               Tjjjb(j4,j2,j3,b)      = VSqjqj(b,j3,b3,j4)*t1b_old(b3,j2)
               PUT Xjjjb(j4,j2,j3,b) += Tjjjb(j4,j2,j3,b)
            ENDDO j2  
      ENDPARDO j3, j4, b, b3 
      execute sip_barrier 
#............................................
      PARDO b, j2, j3, j4
            GET Xjjjb(j4,j2,j3,b) 
            DO j
               GET                 Xjjjj(j,j3,j2,j4) 
               tjb(j,b)          = Xjjjj(j,j3,j2,j4)*Xjjjb(j4,j2,j3,b)
               tjb(j,b)         *= 0.25
               PUT l1b_old_x(j,b) += tjb(j,b)
            ENDDO j
      ENDPARDO b, j2, j3, j4
#     Second piece
      create Xjbii
      create Xjjai
      execute sip_barrier
      delete Xjjjj 
      delete Xjjjb 
#.................................................................
      PARDO j, i2, b1, a1
            REQUEST L2old_ab(i2,a1,j,b1) b1 
            DO i1
               GET t1a_old_x(a1,i1)
               Tjbii(j,b1,i2,i1)      = L2old_ab(i2,a1,j,b1)*t1a_old_x(a1,i1)
               PUT Xjbii(j,b1,i2,i1) += Tjbii(j,b1,i2,i1)
            ENDDO i1
            DO j1
               GET t1b_old_x(b1,j1)
               Tjjai(j1,j,a1,i2)      = L2old_ab(i2,a1,j,b1)*t1b_old_x(b1,j1)
               PUT Xjjai(j1,j,a1,i2) += Tjjai(j1,j,a1,i2)
            ENDDO j1
      ENDPARDO j, i2, b1, a1
      execute sip_barrier
#...........................
      PARDO a, b, b1, i1  
#           Actual term programmed. 
#           T1jbai(j,b,a,i2) = Xjbii(j,b1,i2,i1)*Vbbai(b,b1,a,i1)*t1a_old(a,i2)
            REQUEST Vbbai(b,b1,a,i1) i1 
            DO j
            DO i2
               GET                 Xjbii(j,b1,i2,i1)
               GET                 t1a_old(a,i2)
               Tjibi(j,i2,b1,i1) = Xjbii(j,b1,i2,i1)
               Tbiba(b1,i1,b,a)  = Vbbai(b,b1,a,i1)
               Tjiba(j,i2,b,a)   = Tjibi(j,i2,b1,i1)*Tbiba(b1,i1,b,a)
               Tjbai(j,b,a,i2)   = Tjiba(j,i2,b,a)
               tjb(j,b)          = Tjbai(j,b,a,i2)*t1a_old(a,i2)
               tjb(j,b)         *= -1.0
               PUT l1b_old_x(j,b) += tjb(j,b)
            ENDDO i2
            ENDDO j
      ENDPARDO a, b, b1, i1  
#.............................................
      PARDO a, b, j1, a1  
#           Actual term programmed. 
#           T1jbai(j,b,a,i2)  = Xjjai(j1,j,a1,i2)*Vaabj(a,a1,b,j1)*t1a_old(a,i2) 
            REQUEST Vaabj(a,a1,b,j1) j1 
            DO j
            DO i2  
               GET                 Xjjai(j1,j,a1,i2)
               GET                 t1a_old(a,i2)

               Tjija(j,i2,j1,a1) = Xjjai(j1,j,a1,i2)
               Tjaba(j1,a1,b,a)  = Vaabj(a,a1,b,j1)

               Tjiba(j,i2,b,a)   = Tjija(j,i2,j1,a1)*Tjaba(j1,a1,b,a)
               Tjbai(j,b,a,i2)   = Tjiba(j,i2,b,a)

               tjb(j,b)          = Tjbai(j,b,a,i2)*t1a_old(a,i2)
               tjb(j,b)         *= -1.0
               PUT l1b_old_x(j,b) += tjb(j,b)
            ENDDO i2  
            ENDDO j
      ENDPARDO a, b, j1, a1  
      execute sip_barrier
      delete Xjbii
      delete Xjjai
# 'Fix' the final abomination 
# --------------------------- 
      create Xjjii 
      create Xjbii 
      execute sip_barrier 
      PARDO j1, i1, b1, a1  
            REQUEST Tau_ab_x(a1,i1,b1,j1) j1 
            DO j
            DO i2  
               REQUEST                  L2old_ab(i2,a1,j,b1) b1 
               Tjjii(j,j1,i2,i1)      = L2old_ab(i2,a1,j,b1)*Tau_ab_x(a1,i1,b1,j1)
               PUT Xjjii(j,j1,i2,i1) += Tjjii(j,j1,i2,i1)
            ENDDO i2  
            ENDDO j
      ENDPARDO j1, i1, b1, a1  
#..........................
      PARDO j1, i1, b, a  
            REQUEST Vpiqj(a,i1,b,j1) j1 
            DO i2  
               GET                      t1a_old(a,i2)
               Tjbii(j1,b,i1,i2)      = Vpiqj(a,i1,b,j1)*t1a_old(a,i2)
               PUT Xjbii(j1,b,i1,i2) += Tjbii(j1,b,i1,i2)
            ENDDO i2  
      ENDPARDO j1, i1, b, a  
      execute sip_barrier 
#....................................
      PARDO b, i1, i2, j1  
            GET Xjbii(j1,b,i1,i2) 
            DO j
               GET                 Xjjii(j,j1,i2,i1) 
               tjb(j,b)          = Xjjii(j,j1,i2,i1)*Xjbii(j1,b,i1,i2)
               PUT l1b_old_x(j,b) += tjb(j,b)
            ENDDO j
      ENDPARDO b, i1, i2, j1 
      execute sip_barrier 
      delete Xjjii 
      delete Xjbii 
      ENDPROC L1WABCDb 
