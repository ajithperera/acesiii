               GET t1b_old_x(b1,j)
               T1jjpp(j1,j,a,a1) = Vaabj(a1,a,b1,j1)*t1b_old_x(b1,j)
               Tjjpp(j1,j,a,a1) += T1jjpp(j1,j,a,a1)
            ENDDO b1
            DO i1
               REQUEST Vpiqj(a1,i1,j,j1) j1
               GET t1a_old(a,i1)
               GET t1a_old_x(a,i1)
               T1jjpp(j1,j,a,a1) = Vpiqj(a1,i1,j,j1)*t1a_old_x(a,i1)
               Tjjpp(j1,j,a,a1) -= T1jjpp(j1,j,a,a1)
               DO b1
                  REQUEST T2old_ab_x(a,i1,b1,j) j
                  REQUEST Vpiqj(a1,i1,b1,j1)  j1
                  GET t1b_old(b1,j)
                  GET t1b_old_x(b1,j)

                  Taibj(a,i1,b1,j)   = t1b_old_x(b1,j)^t1a_old(a,i1)
                  T1aibj(a,i1,b1,j)  = T2old_ab_x(a,i1,b1,j)
                  T1aibj(a,i1,b1,j) += Taibj(a,i1,b1,j)

                  Taibj(a,i1,b1,j)   = t1b_old(b1,j)^t1a_old_x(a,i1)
                  T1aibj(a,i1,b1,j) += Taibj(a,i1,b1,j)

                  T1jjpp(j1,j,a,a1) = T1aibj(a,i1,b1,j)*Vpiqj(a1,i1,b1,j1)
                  Tjjpp(j1,j,a,a1) -= T1jjpp(j1,j,a,a1)
               ENDDO b1
            ENDDO i1
            Tjjpp(j1,j,a,a1)            *= -1.0
            PREPARE W1mjbe_ba_x(j1,j,a,a1) = Tjjpp(j1,j,a,a1)
      ENDPARDO j, j1, a1, a

      ENDPROC W1MJBEba_S_x
#     --------------------

      PROC W1MJBEba
