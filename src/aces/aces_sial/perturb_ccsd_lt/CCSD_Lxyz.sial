     PROC L2NEWAA_X
#     --------------

      PARDO a, a1, a2, i1              
            REQUEST VSaaai(a,a2,a1,i1)  i1
            DO i
               GET                            l1a_old_x(i,a2)
               Tiaia(i,a,i1,a1)             = VSaaai(a,a2,a1,i1)*l1a_old_x(i,a2)
               T3iaia(i1,a,i,a1)            = Tiaia(i,a,i1,a1)
               T3iaia(i1,a,i,a1)           *= -1.0
               PREPARE L2new_aa_x(i,a,i1,a1) +=  Tiaia(i,a,i1,a1)
               PREPARE L2new_aa_x(i1,a,i,a1) += T3iaia(i1,a,i,a1)
            ENDDO i
      ENDPARDO a, a1, a2, i1
#............................................
      PARDO i, a1, a, i1            
            REQUEST             VSpipi(a,i,a1,i1) i1
            GET                 l1a_old_x(i,a)
            GET                 Fme_a(i1,a1)

#pv         Tiaia(i,a,i1,a1)  = VSpipi(a,i,a1,i1)
            Tiaia(i,a,i1,a1)  = 0.0
            T2iaia(i,a1,i1,a) = 0.0
            T3iaia(i1,a,i,a1) = 0.0
            T4iaia(i1,a1,i,a) = 0.0
            DO a2
               REQUEST              L2old_aa_x(i,a,i1,a2) a2
               REQUEST              VSpipi(a,i,a2,i1)   i1
               GET                  Gae_a_x(a1,a2)
               GET                  F1ae_a(a2,a1)

               T1iaia(i,a,i1,a1)  = L2old_aa_x(i,a,i1,a2)*F1ae_a(a2,a1)
               Tiaia(i,a,i1,a1)  += T1iaia(i,a,i1,a1)

               TXiaia(i,a1,i1,a)  = T1iaia(i,a,i1,a1)
               T2iaia(i,a1,i1,a) -= TXiaia(i,a1,i1,a)

               Taa(a2,a1)         = Gae_a_x(a1,a2)

               DO i2
                  GET           l1a_old_x(i2,a1)
                  GET           t1a_old(a2,i2)

                  T1aa(a2,a1) = t1a_old(a2,i2)*l1a_old_x(i2,a1)
                  Taa(a2,a1) -= T1aa(a2,a1)
               ENDDO i2
               T1iaia(i,a,i1,a1)  = VSpipi(a,i,a2,i1)*Taa(a2,a1)
               Tiaia(i,a,i1,a1)  += T1iaia(i,a,i1,a1)

               TXiaia(i,a1,i1,a)  = T1iaia(i,a,i1,a1)
               T2iaia(i,a1,i1,a) -= TXiaia(i,a1,i1,a)
            ENDDO a2
            T1iaia(i,a,i1,a1)  = l1a_old_x(i,a)^Fme_a(i1,a1)
            Tiaia(i,a,i1,a1)  += T1iaia(i,a,i1,a1)

            TXiaia(i,a1,i1,a)  = T1iaia(i,a,i1,a1)
            T2iaia(i,a1,i1,a) -= TXiaia(i,a1,i1,a)

            TXiaia(i1,a,i,a1)  = T1iaia(i,a,i1,a1)
            T3iaia(i1,a,i,a1) -= TXiaia(i1,a,i,a1)

            TXiaia(i1,a1,i,a)  = T1iaia(i,a,i1,a1)
            T4iaia(i1,a1,i,a) += TXiaia(i1,a1,i,a)

            PREPARE L2new_aa_x(i,a,i1,a1) +=  Tiaia(i,a,i1,a1)
            PREPARE L2new_aa_x(i,a1,i1,a) += T2iaia(i,a1,i1,a)
            PREPARE L2new_aa_x(i1,a,i,a1) += T3iaia(i1,a,i,a1)
            PREPARE L2new_aa_x(i1,a1,i,a) += T4iaia(i1,a1,i,a)
      ENDPARDO i, a1, a, i1
#..............................................
      PARDO i1, i, a, a1
            Tiaia(i,a,i1,a1)  = 0.0
            T2iaia(i,a1,i1,a) = 0.0
            T3iaia(i1,a,i,a1) = 0.0
            DO i2
               REQUEST L2old_aa_x(i,a,i2,a1) a1
               REQUEST VSpipi(i2,i,a1,i1)  i1
               REQUEST VSpipi(a,i,a1,i2)   i2
               GET Gmi_a_x(i2,i1)
               GET F1mi_a(i1,i2)
               GET l1a_old_x(i2,a)

               T1iaia(i,a,i1,a1)  = L2old_aa_x(i,a,i2,a1)*F1mi_a(i1,i2)
               T1iaia(i,a,i1,a1) *= -1.0

               Tii(i1,i2)         = Gmi_a_x(i2,i1)
               Tiaia(i,a,i1,a1)  += T1iaia(i,a,i1,a1)

               TXiaia(i1,a,i,a1)  = T1iaia(i,a,i1,a1)
               T3iaia(i1,a,i,a1) -= TXiaia(i1,a,i,a1)

               T1iaia(i,a,i1,a1)  = VSpipi(i2,i,a1,i1)*l1a_old_x(i2,a)
               Tiaia(i,a,i1,a1)  -= T1iaia(i,a,i1,a1)

               TXiaia(i,a1,i1,a)  = T1iaia(i,a,i1,a1)
               T2iaia(i,a1,i1,a) += TXiaia(i,a1,i1,a)

               DO a2
                  GET l1a_old_x(i1,a2)
                  GET t1a_old(a2,i2)

                  T1ii(i1,i2) = l1a_old_x(i1,a2)*t1a_old(a2,i2)
                  Tii(i1,i2) += T1ii(i1,i2)
               ENDDO a2
               T1iaia(i,a,i1,a1)  = VSpipi(a,i,a1,i2)*Tii(i1,i2)
               Tiaia(i,a,i1,a1)  -= T1iaia(i,a,i1,a1)

               TXiaia(i1,a,i,a1)  = T1iaia(i,a,i1,a1)
               T3iaia(i1,a,i,a1) += TXiaia(i1,a,i,a1)
               DO i3
                  REQUEST L2old_aa_x(i2,a,i3,a1)  a1
                  GET     W1minj_aa(i,i2,i1,i3)
                  T1iaia(i,a,i1,a1)  = W1minj_aa(i,i2,i1,i3)*L2old_aa_x(i2,a,i3,a1)
                  T1iaia(i,a,i1,a1) *= 0.5
                  Tiaia(i,a,i1,a1)  += T1iaia(i,a,i1,a1)
               ENDDO i3
            ENDDO i2
            PREPARE L2new_aa_x(i,a,i1,a1) +=  Tiaia(i,a,i1,a1)
            PREPARE L2new_aa_x(i,a1,i1,a) += T2iaia(i,a1,i1,a)
            PREPARE L2new_aa_x(i1,a,i,a1) += T3iaia(i1,a,i,a1)
      ENDPARDO i1, i, a, a1
      ENDPROC L2NEWAA_X
#     ------------------

      PROC L2OLDAA_X
#     --------------
#pv      PARDO a, a1, a2, i1 # VFL 
#pv            REQUEST VSaaai(a,a2,a1,i1)  i1  
#pv            DO i
#pv               GET                            l1a_old(i,a2)
#pv               Tiaia(i,a,i1,a1)             = VSaaai(a,a2,a1,i1)*l1a_old(i,a2)
#pv               T3iaia(i1,a,i,a1)            = Tiaia(i,a,i1,a1)
#pv            T3iaia(i1,a,i,a1)           *= -1.0  
#pv            PREPARE L2new_aa(i,a,i1,a1) +=  Tiaia(i,a,i1,a1)
#pv            PREPARE L2new_aa(i1,a,i,a1) += T3iaia(i1,a,i,a1)
#pv         ENDDO i
#pv   ENDPARDO a, a1, a2, i1
#...............................................
      PARDO i, a1, a, i1 # VFL 
#pv         REQUEST             VSpipi(a,i,a1,i1) i1  
            GET                 l1a_old(i,a)
            GET                 Fme_a_x(i1,a1)
#pv         Tiaia(i,a,i1,a1)  = VSpipi(a,i,a1,i1)
            Tiaia(i,a,i1,a1)  = 0.0
            T2iaia(i,a1,i1,a) = 0.0
            T3iaia(i1,a,i,a1) = 0.0
            T4iaia(i1,a1,i,a) = 0.0
            DO a2
               REQUEST              L2old_aa(i,a,i1,a2) a2 
               REQUEST              VSpipi(a,i,a2,i1)   i1  
#pv            GET                  Gae_a(a1,a2)
               GET                  F1ae_a_x(a2,a1)

               T1iaia(i,a,i1,a1)  = L2old_aa(i,a,i1,a2)*F1ae_a_x(a2,a1)
               Tiaia(i,a,i1,a1)  += T1iaia(i,a,i1,a1)

               TXiaia(i,a1,i1,a)  = T1iaia(i,a,i1,a1)
               T2iaia(i,a1,i1,a) -= TXiaia(i,a1,i1,a)

#pv            Taa(a2,a1)         = Gae_a(a1,a2)
               Taa(a2,a1)         = 0.0
               DO i2
                  GET           l1a_old(i2,a1)
                  GET           t1a_old_x(a2,i2)

                  T1aa(a2,a1) = t1a_old_x(a2,i2)*l1a_old(i2,a1)
                  Taa(a2,a1) -= T1aa(a2,a1)
               ENDDO i2
               T1iaia(i,a,i1,a1)  = VSpipi(a,i,a2,i1)*Taa(a2,a1)
               Tiaia(i,a,i1,a1)  += T1iaia(i,a,i1,a1)

               TXiaia(i,a1,i1,a)  = T1iaia(i,a,i1,a1)
               T2iaia(i,a1,i1,a) -= TXiaia(i,a1,i1,a)
            ENDDO a2

            T1iaia(i,a,i1,a1)  = l1a_old(i,a)^Fme_a_x(i1,a1)
            Tiaia(i,a,i1,a1)  += T1iaia(i,a,i1,a1)

            TXiaia(i,a1,i1,a)  = T1iaia(i,a,i1,a1)
            T2iaia(i,a1,i1,a) -= TXiaia(i,a1,i1,a)

            TXiaia(i1,a,i,a1)  = T1iaia(i,a,i1,a1)
            T3iaia(i1,a,i,a1) -= TXiaia(i1,a,i,a1)

            TXiaia(i1,a1,i,a)  = T1iaia(i,a,i1,a1)
            T4iaia(i1,a1,i,a) += TXiaia(i1,a1,i,a)

            PREPARE L2old_aa_x(i,a,i1,a1) +=  Tiaia(i,a,i1,a1)
            PREPARE L2old_aa_x(i,a1,i1,a) += T2iaia(i,a1,i1,a)
            PREPARE L2Old_aa_x(i1,a,i,a1) += T3iaia(i1,a,i,a1)
            PREPARE L2old_aa_x(i1,a1,i,a) += T4iaia(i1,a1,i,a)
      ENDPARDO i, a1, a, i1
#.........................................................
      PARDO i1, i, a, a1
            Tiaia(i,a,i1,a1)  = 0.0  
            T2iaia(i,a1,i1,a) = 0.0
            T3iaia(i1,a,i,a1) = 0.0
            DO i2
               REQUEST L2old_aa(i,a,i2,a1) a1
               REQUEST VSpipi(i2,i,a1,i1)  i1  
               REQUEST VSpipi(a,i,a1,i2)   i2  
#pv            GET Gmi_a(i2,i1)
               GET F1mi_a_x(i1,i2)
               GET l1a_old(i2,a)

               T1iaia(i,a,i1,a1)  = L2old_aa(i,a,i2,a1)*F1mi_a_x(i1,i2)
               T1iaia(i,a,i1,a1) *= -1.0

#pv            Tii(i1,i2)         = Gmi_a(i2,i1)
               Tiaia(i,a,i1,a1)  += T1iaia(i,a,i1,a1)
               TXiaia(i1,a,i,a1)  = T1iaia(i,a,i1,a1)
               T3iaia(i1,a,i,a1) -= TXiaia(i1,a,i,a1)

#pv            T1iaia(i,a,i1,a1)  = VSpipi(i2,i,a1,i1)*l1a_old(i2,a)
#pv            Tiaia(i,a,i1,a1)  -= T1iaia(i,a,i1,a1)
#pv            TXiaia(i,a1,i1,a)  = T1iaia(i,a,i1,a1)
#pv            T2iaia(i,a1,i1,a) += TXiaia(i,a1,i1,a)

               Tii(i1,i2)         = 0.0
               DO a2
                  GET l1a_old(i1,a2)
                  GET t1a_old_x(a2,i2)
                  T1ii(i1,i2) = l1a_old(i1,a2)*t1a_old_x(a2,i2)
                  Tii(i1,i2) += T1ii(i1,i2)
               ENDDO a2
               T1iaia(i,a,i1,a1)  = VSpipi(a,i,a1,i2)*Tii(i1,i2)
               Tiaia(i,a,i1,a1)  -= T1iaia(i,a,i1,a1)

               TXiaia(i1,a,i,a1)  = T1iaia(i,a,i1,a1)
               T3iaia(i1,a,i,a1) += TXiaia(i1,a,i,a1)
               DO i3
                  REQUEST L2old_aa(i2,a,i3,a1)  a1 
                  GET     W1minj_aa_x(i,i2,i1,i3) 
                  T1iaia(i,a,i1,a1)  = W1minj_aa_x(i,i2,i1,i3)*L2old_aa(i2,a,i3,a1)
                  T1iaia(i,a,i1,a1) *= 0.5
                  Tiaia(i,a,i1,a1)  += T1iaia(i,a,i1,a1)
               ENDDO i3
            ENDDO i2
            PREPARE L2old_aa_x(i,a,i1,a1) +=  Tiaia(i,a,i1,a1)
            PREPARE L2old_aa_x(i,a1,i1,a) += T2iaia(i,a1,i1,a)
            PREPARE L2old_aa_x(i1,a,i,a1) += T3iaia(i1,a,i,a1)
      ENDPARDO i1, i, a, a1

      ENDPROC L2OLDAA_X
