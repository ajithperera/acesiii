               GET t1b_old_x(b2,j)
               T1jbbj(j1,b1,b,j)  = VSbbbj(b2,b,b1,j1)*t1b_old_x(b2,j)
               Tjbbj(j1,b1,b,j)  += T1jbbj(j1,b1,b,j)
            ENDDO b2
