               REQUEST L2old_ab(i,a,j,b) b 

               T1iajb(i,a,j1,b1)            = L2old_ab(i,a,j,b)*W1MEBJ_BB_X(j1,b1,b,j)
               PREPARE L2old_ab_x(i,a,j1,b1) += T1iajb(i,a,j1,b1)
            ENDDO a
            ENDDO i
