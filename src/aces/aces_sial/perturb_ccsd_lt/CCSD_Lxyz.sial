           t1jb(j,b)     = d3jb(j,b) 
           t1jb(j,b)    += e4jb(j,b) 
           t1jb(j,b)    *= c4  
           tjb(j,b)     += t1jb(j,b) 
# 
           t1jb(j,b)     = d4jb(j,b) 
           t1jb(j,b)    += e5jb(j,b) 
           t1jb(j,b)    *= c5  
           tjb(j,b)     += t1jb(j,b) 
# 
           PUT l1b_old_x(j,b) = tjb(j,b) 
# 
     ENDPARDO b, j 
     execute server_barrier 
     execute sip_barrier 
     ENDPROC DIIS4  
