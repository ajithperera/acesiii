                  T1aibj(a1,i,b,j1) += Taibj(a1,i,b,j1)


                  T1iiqq(i1,i,b,b1)  = T1aibj(a1,i,b,j1)*Vpiqj(a1,i1,b1,j1)
                  Tiiqq(i1,i,b,b1)  -= T1iiqq(i1,i,b,b1)
               ENDDO a1
            ENDDO j1
            Tiiqq(i1,i,b,b1)            *= -1.0
            PREPARE W1mjbe_ab_x(i1,i,b,b1) = Tiiqq(i1,i,b,b1)
      ENDPARDO i, i1, b1, b
      ENDPROC W1MJBEab_S_x
