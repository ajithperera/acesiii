               REQUEST L2old_ab_x(i,a,j,b) b

               T1iajb(i,a,j1,b1)            = L2old_ab_x(i,a,j,b)*W1MEBJ_BB(j1,b1,b,j)
               PREPARE L2new_ab_x(i,a,j1,b1) += T1iajb(i,a,j1,b1)
