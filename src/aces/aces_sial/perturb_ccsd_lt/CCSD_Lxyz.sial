      PROC W1MEBJAA 
#     -------------
      PARDO i1, a1, a, i 
            REQUEST W1MEBJ_AA_x(i1,a1,a,i) a 

# Compute contribution to L1a_new  
            GET l1a_old(i,a)
            Tia(i1,a1)          = W1MEBJ_AA_X(i1,a1,a,i)*l1a_old(i,a)
            PUT l1a_old_x(i1,a1) += Tia(i1,a1)
# Compute contributions to L2new_aa  
            DO i2
            DO a2
               REQUEST L2old_aa(i2,a2,i,a) a 
               T1iaia(i2,a2,i1,a1)        = L2old_aa(i2,a2,i,a)*W1MEBJ_AA_X(i1,a1,a,i)
               T2iaia(i2,a1,i1,a2)        = T1iaia(i2,a2,i1,a1)
               T2iaia(i2,a1,i1,a2)       *= -1.0  
               T3iaia(i1,a2,i2,a1)        = T1iaia(i2,a2,i1,a1)
               T3iaia(i1,a2,i2,a1)       *= -1.0  
               T4iaia(i1,a1,i2,a2)        = T1iaia(i2,a2,i1,a1)

               PREPARE L2old_aa_x(i2,a2,i1,a1) += T1iaia(i2,a2,i1,a1) 
               PREPARE L2old_aa_x(i2,a1,i1,a2) += T2iaia(i2,a1,i1,a2) 
               PREPARE L2old_aa_x(i1,a2,i2,a1) += T3iaia(i1,a2,i2,a1)  
               PREPARE L2old_aa_x(i1,a1,i2,a2) += T4iaia(i1,a1,i2,a2)  
            ENDDO a2
            ENDDO i2
# Compute contributions to L2new_ab 
            DO j
            DO b
               REQUEST L2old_ab(i,a,j,b) b 
               T1iajb(i1,a1,j,b)      = L2old_ab(i,a,j,b)*W1MEBJ_AA_x(i1,a1,a,i)
               PREPARE L2old_ab_x(i1,a1,j,b) += T1iajb(i1,a1,j,b)
            ENDDO b
            ENDDO j
      ENDPARDO i1, a1, a, i 
      ENDPROC W1MEBJAA 
