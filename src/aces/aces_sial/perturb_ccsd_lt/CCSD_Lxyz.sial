               T1iajb(i,a1,j1,b)            = L2old_ab_x(i,a,j,b)*W1mjbe_ba(j1,j,a,a1)
               PREPARE L2new_ab_x(i,a1,j1,b) += T1iajb(i,a1,j1,b)  # VFL
