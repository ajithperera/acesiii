           t2iaia(i,a,i1,a1)            = d0iaia(i,a,i1,a1)
           t2iaia(i,a,i1,a1)           += e1iaia(i,a,i1,a1)
           t2iaia(i,a,i1,a1)           *= c1
           tiaia(i,a,i1,a1)             = t2iaia(i,a,i1,a1)
#
           t2iaia(i,a,i1,a1)            = d1iaia(i,a,i1,a1)
           t2iaia(i,a,i1,a1)           += e2iaia(i,a,i1,a1)
           t2iaia(i,a,i1,a1)           *= c2
           tiaia(i,a,i1,a1)            += t2iaia(i,a,i1,a1)
