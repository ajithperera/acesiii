               Tiabj(i,a,b,j2)   = Tijab(i,j2,a,b)

               tia(i,a)          = Tiabj(i,a,b,j2)*t1b_old(b,j2)
               tia(i,a)         *= -1.0
               PUT l1a_old_x(i,a) += tia(i,a)
            ENDDO j2
            ENDDO i
