               Tjbjb(j,b,j2,b3)  = Xjjjb(j,j3,j2,b2)*VSbbbj(b3,b2,b,j3)
               tjb(j,b)          = Tjbjb(j,b,j2,b3)*t1b_old_x(b3,j2)
               tjb(j,b)         *= 0.5
               PUT l1b_old_x(j,b) += tjb(j,b)
            ENDDO j
            ENDDO j2
      ENDPARDO b, b3, b2, j3
      execute sip_barrier
      delete Xjjjb
# 'Fix' on 9/29/08 
# ---------------- 
      create Xjjjj
      create Xjjjb
      execute sip_barrier
#..................................................................................
      PARDO j3, j4, b1, b2
            REQUEST Tau_bb(b1,j3,b2,j4) j4
            DO j
            DO j2
               REQUEST                  L2old_bb(j,b1,j2,b2) b2
               Tjjjj(j,j3,j2,j4)      = L2old_bb(j,b1,j2,b2)*Tau_bb(b1,j3,b2,j4)
               PUT Xjjjj(j,j3,j2,j4) += Tjjjj(j,j3,j2,j4)
            ENDDO j2
            ENDDO j
      ENDPARDO j3, j4, b1, b2
#......................................
      PARDO j3, j4, b, b3
            REQUEST VSqjqj(b,j3,b3,j4) j4
            DO j2
               GET                      t1b_old_x(b3,j2)
               Tjjjb(j4,j2,j3,b)      = VSqjqj(b,j3,b3,j4)*t1b_old_x(b3,j2)
               PUT Xjjjb(j4,j2,j3,b) += Tjjjb(j4,j2,j3,b)
            ENDDO j2
      ENDPARDO j3, j4, b, b3
      execute sip_barrier
#............................................
      PARDO b, j2, j3, j4
            GET Xjjjb(j4,j2,j3,b)
            DO j
               GET                 Xjjjj(j,j3,j2,j4)
               tjb(j,b)          = Xjjjj(j,j3,j2,j4)*Xjjjb(j4,j2,j3,b)
               tjb(j,b)         *= 0.25
               PUT l1b_old_x(j,b) += tjb(j,b)
            ENDDO j
      ENDPARDO b, j2, j3, j4
#     Second piece
      create Xjbii
      create Xjjai
      execute sip_barrier
      delete Xjjjj
      delete Xjjjb
#.................................................................
      PARDO j, i2, b1, a1
            REQUEST L2old_ab(i2,a1,j,b1) b1
            DO i1
               GET t1a_old(a1,i1)
               Tjbii(j,b1,i2,i1)      = L2old_ab(i2,a1,j,b1)*t1a_old(a1,i1)
               PUT Xjbii(j,b1,i2,i1) += Tjbii(j,b1,i2,i1)
            ENDDO i1
            DO j1
               GET t1b_old(b1,j1)
               Tjjai(j1,j,a1,i2)      = L2old_ab(i2,a1,j,b1)*t1b_old(b1,j1)
               PUT Xjjai(j1,j,a1,i2) += Tjjai(j1,j,a1,i2)
            ENDDO j1
      ENDPARDO j, i2, b1, a1
      execute sip_barrier
#...........................
      PARDO a, b, b1, i1
#           Actual term programmed. 
#           T1jbai(j,b,a,i2) = Xjbii(j,b1,i2,i1)*Vbbai(b,b1,a,i1)*t1a_old(a,i2)
            REQUEST Vbbai(b,b1,a,i1) i1
            DO j
            DO i2
               GET                 Xjbii(j,b1,i2,i1)
               GET                 t1a_old_x(a,i2)
               Tjibi(j,i2,b1,i1) = Xjbii(j,b1,i2,i1)
               Tbiba(b1,i1,b,a)  = Vbbai(b,b1,a,i1)
               Tjiba(j,i2,b,a)   = Tjibi(j,i2,b1,i1)*Tbiba(b1,i1,b,a)
               Tjbai(j,b,a,i2)   = Tjiba(j,i2,b,a)
               tjb(j,b)          = Tjbai(j,b,a,i2)*t1a_old_x(a,i2)
               tjb(j,b)         *= -1.0
               PUT l1b_old_x(j,b) += tjb(j,b)
            ENDDO i2
            ENDDO j
      ENDPARDO a, b, b1, i1
#.............................................
      PARDO a, b, j1, a1
