           REQUEST e1iajb(i,a,j,b) a # kiter-2 amplitudes
           REQUEST e2iajb(i,a,j,b) a # kiter-1 amplitudes
           REQUEST e3iajb(i,a,j,b) a # kiter-0 amplitudes
