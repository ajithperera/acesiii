               PREPARE L2new_bb_x(j2,b1,j1,b2) += T2jbjb(j2,b1,j1,b2)
               PREPARE L2new_bb_x(j1,b2,j2,b1) += T3jbjb(j1,b2,j2,b1)
               PREPARE L2new_bb_x(j1,b1,j2,b2) += T4jbjb(j1,b1,j2,b2)
            ENDDO b2
            ENDDO j2
# Compute contributions to L2new_ab 
            DO i
